[MESSAGES CONTROL]
disable=too-many-arguments,
  too-many-locals,
  too-many-return-statements,
  too-many-branches,
  too-many-instance-attributes,
  too-many-statements,
  invalid-name,
  undefined-variable,
  duplicate-code,
  consider-using-with,
  consider-using-dict-items

[FORMAT]
max-line-length=100
indent-string='    '
init-import=yes 

[REFACTORING]
max-nested-blocks=6
